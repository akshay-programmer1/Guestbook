<resource-action-mapping>
    <model-resource>
        <model-name>com.liferay.docs.guestbook</model-name>
        <portlet-ref>
            <portlet-name>com_liferay_docs_guestbook_portlet_GuestbookPortlet</portlet-name>
        </portlet-ref>
        <root>true</root>
        <weight>1</weight>
        <permissions>
            <supports>
                <action-key>VIEW</action-key>
            </supports>
            <site-member-defaults />
            <guest-defaults />
            <guest-unsupported />
        </permissions>
    </model-resource>


    <model-resource>
        <model-name>com.liferay.docs.guestbook.model.GuestbookEntry</model-name>
        <portlet-ref>
            <portlet-name>com_liferay_docs_guestbook_portlet_GuestbookPortlet</portlet-name>
        </portlet-ref>
        <weight>2</weight>
        <permissions>
            <supports>
                <action-key>Guestbook_ADD_OK</action-key>

            </supports>
            <site-member-defaults />
            <guest-defaults />
            <guest-unsupported />
        </permissions>
    </model-resource>

    <model-resource>
        <model-name>com.liferay.docs.guestbook.model.Guestbook</model-name>
        <portlet-ref>
            <portlet-name>com_liferay_docs_guestbook_portlet_GuestbookPortlet</portlet-name>
        </portlet-ref>
        <weight>3</weight>
        <permissions>
            <supports>
                <action-key>Guestbook_DELETE_OK</action-key>

            </supports>
            <site-member-defaults />
            <guest-defaults />
            <guest-unsupported />
        </permissions>
    </model-resource>

</resource-action-mapping>




